package com.snd.CanvasGoogleConnection.CanvasConnection;

public class CanvasAssignment 
{
	private int id;
	private String dueDate;
	private String url;
	private String name;
	private String course;
	
	public CanvasAssignment(int id, String dueDate, String url, String name, String course)
	{
		this.id = id;
		this.dueDate = dueDate;
		this.url = url;
		this.name = name;
		this.course = course;
	}
	
	public int getId() {
	    return id;
	}

	public String getDueDate() {
	    return dueDate;
	}

	public String getUrl() {
	    return url;
	}

	public String getName() {
	    return name;
	}

	public String getCourse() {
	    return course;
	}
	
	public String toString()
	{
		return name + " " + id + " " + course + " " + url;
	}
}
